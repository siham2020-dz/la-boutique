<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap Demo</title>
    
    <!-- Ajout de Bootstrap -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    
    <style>
        .text-center {
            text-align: center;
        }
         .text-right{
            text-align: right;
        }
        .text-left{
            text-align: left;
        }
        .body{
            font-family:Roboto,sans-serif;
        }
        table {
            border-collapse: collapse;
        }
        table td{
            padding: 0.5rem;
            border-top: 1px solid #ccc;
        }
    </style>
</head>
<body>

   
     <div class="text-center">
    <img src="data:image/jpeg;base64,">

   
    <h3>Facture commande  n°{{ order.id }}<</h3>
        <table style="width: 100%; border-bottom: 1px solid gray;">
            <tr>
            <td style="width: 49%">
                <strong> La Boutique Française</strong><br/>
                  mon adresse.<br/>
                 code postal.ville.france.<br/>
                 email@laboutiquefrançaise@gmail.com
                 0302050104
            </td>
              <td class="text-right">
                {{ order.delivery|raw }}
              
              </td>      
        
        </tr>
        
        </table>>
    </div>
<h5>Le Contenue de ma Commande</h5>
<table style="width:100%;">
  <thead>
    <tr>
     
    
      <th scope="col" style="text-left">Produit</th>
      <th scope="col" style="text-left">Quantité</th>
      <th scope="col" style="text-left">Prix unitaire H.</th>
      <th scope="col" style="text-left">TVA</th>
      <th scope="col" class="text-right">Total T.T.C </th>

    </tr>
  </thead>
  <tbody>
  {% for product in order.orderDetails%}

<tr>

    
     <td>{{ product.ProductName}}</td>
    <td>{{ product.productQuantity}}</td>
    <td>{{ product.productPrice|price}}</td>
    <td>{{ product.productTva}}%</td>
    {% set coeff= 1+(product.productTva /100) %}
    <td class="text-right">{{(product.productQuantity*(product.productPrice*coeff))|price}}</td>
     

    </tr>
{% endfor %}
    
  <tr>
 
   <td>{{ order.carrierName}}</td>
   <td>1</td>
   <td></td>
   <td></td>
   <td>{{order.carrierPrice |price}}</td>
  </tr>
  </tbody>
</table>
<div  class="text-right" style="margin_top:10px;">
<strong>Totale H.C :</strong>{{ (order.totalWt - order.totaltva)|price }}<br/>
<strong>Totale TVA :</strong>{{ order.totaltva|price }}<br/>
<strong>Totale T.T.C :</strong>{{ order.totalWt|price }}

</div>
</body>
</html>
